<Application x:Class="CanonUtility.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:CanonUtility"
             xmlns:conv="clr-namespace:CanonUtility.Converter"
             xmlns:cwc="clr-namespace:CanonWpf.Converter;assembly=CanonWpf"
             StartupUri="View/MainView.xaml">
    <Application.Resources>
        <conv:DebugConverter x:Key="Debug"/>
        <conv:EnumToNameConverter x:Key="EnumToName"/>
        <conv:EnumToDescriptionConverter x:Key="EnumToDescription"/>
        <conv:EnumToNumberConverter x:Key="EnumToNumber"/>
        <conv:EnumToHexConverter x:Key="EnumToHex"/>
        <cwc:ConnectionTypeToImageConverter x:Key="ConnectionTypeToImage"/>
    </Application.Resources>
</Application>
